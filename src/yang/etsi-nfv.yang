  include etsi-nfv-common;
  include etsi-nfv-vnf;
  include etsi-nfv-ns;

  organization
    "European Telecommunications Standards Institute (ETSI)";

  description
    "Network Function Virtualization Orchestrator";

  revision 2017-01-20 {
